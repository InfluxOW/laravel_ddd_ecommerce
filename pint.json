{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "phpdoc_separation": {
            "groups": [
                [
                    "deprecated",
                    "link",
                    "see",
                    "since"
                ],
                [
                    "author",
                    "copyright",
                    "license"
                ],
                [
                    "category",
                    "package",
                    "subpackage"
                ],
                [
                    "property"
                ],
                [
                    "property-read"
                ],
                [
                    "property-write"
                ]
            ]
        },
        "phpdoc_align": {
            "tags": [
                "method",
                "param",
                "property",
                "property-read",
                "property-write",
                "return",
                "throws",
                "type",
                "var"
            ]
        },
        "class_definition": {
            "space_before_parenthesis": true
        },
        "new_with_braces": true
    },
    "exclude": [
    ],
    "cache-file": "storage/pint/pint.json"
}
