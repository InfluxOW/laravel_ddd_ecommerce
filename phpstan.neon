includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    reportUnmatchedIgnoredErrors: false

    paths:
        - app
        - database
        - resources

    # The level 9 is the highest level
    level: 8

    excludePaths:
        - app/Domains/Components/Purchasable/Casts/MoneyCast.php

    ignoreErrors:
        - '#Attribute class JetBrains\\PhpStorm\\ArrayShape does not exist#'
        - '#Access to an undefined property Squire\\Models#'
        - '#Access to an undefined property Illuminate\\Database\\Eloquent\\Model::\$id#'
        - '#Model::make\(\)#'
        - '#Call to an undefined static method Illuminate\\Support\\Facades\\Redis#'
        - '#\(Akaunting\\Money\\Money\) does not accept int#'

    checkMissingIterableValueType: false

    tmpDir: storage/phpstan
