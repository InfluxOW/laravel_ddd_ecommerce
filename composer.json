{
    "name": "influx/laravel-ddd-ecommerce",
    "type": "project",
    "description": "Laravel DDD E-Commerce",
    "keywords": [
        "laravel",
        "ddd",
        "e-commerce"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:InfluxOW/baum.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:InfluxOW/Prequel.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-igbinary": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-phar": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "ext-swoole": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "akaunting/laravel-money": "^3.0",
        "anhskohbo/no-captcha": "^3.4",
        "aws/aws-sdk-php": "^3.209",
        "babenkoivan/elastic-migrations": "^3.1",
        "babenkoivan/elastic-scout-driver": "^3.0",
        "babenkoivan/elastic-scout-driver-plus": "^4.0",
        "baum/baum": "dev-add_laravel_9_support",
        "darkaonline/l5-swagger": "^8.3.0",
        "fakerphp/faker": "^1.9.1",
        "filament/filament": "^2.10",
        "filament/spatie-laravel-media-library-plugin": "^2.10",
        "filament/spatie-laravel-settings-plugin": "^2.10",
        "flowframe/laravel-trend": "^0.1",
        "fruitcake/laravel-cors": "^3.0",
        "geoip2/geoip2": "^2.12",
        "guzzlehttp/guzzle": "^7.4.1",
        "humaidem/filament-map-picker": "^0.1.3",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.9",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^2.14",
        "laravel/scout": "^9.4",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mbezhanov/faker-provider-collection": "^2.0",
        "mstaack/laravel-postgis": "^5.4",
        "paulhennell/faker-news": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "protoqol/prequel": "dev-main",
        "sentry/sentry-laravel": "^2.12",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-sluggable": "^3.3",
        "squirephp/countries-en": "^3.3",
        "squirephp/regions-en": "^3.3",
        "stevebauman/purify": "^4.0",
        "studio/laravel-totem": "^9.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "torann/geoip": "^3.0",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.11",
        "brianium/paratest": "^6.4",
        "itsgoingd/clockwork": "^5.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "laravel/pint": "^0.1.3",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0",
        "phpstan/phpstan": "^1.4",
        "roave/security-advisories": "dev-latest",
        "slevomat/coding-standard": "^8.0",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "\\": [
                "app/Domains/Catalog/Database/Elastic",
                "app/Domains/Feedback/Database/Elastic",
                "app/Domains/Generic/Database/Elastic",
                "app/Domains/News/Database/Elastic",
                "app/Domains/Users/Database/Elastic"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php -r \"copy('.githooks/pre-commit', '.git/hooks/pre-commit');\"",
            "@php artisan livewire:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan filament:upgrade",
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
