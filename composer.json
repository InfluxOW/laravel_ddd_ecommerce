{
    "name": "influx/laravel-ddd-ecommerce",
    "description": "Laravel DDD E-Commerce",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "ddd",
        "e-commerce"
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-igbinary": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-phar": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "ext-swoole": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "akaunting/laravel-money": "^4.0",
        "anhskohbo/no-captcha": "^3.4",
        "aws/aws-sdk-php": "^3.209",
        "babenkoivan/elastic-migrations": "^3.1",
        "babenkoivan/elastic-scout-driver": "^3.0",
        "babenkoivan/elastic-scout-driver-plus": "^4.0",
        "baum/baum": "dev-main",
        "darkaonline/l5-swagger": "^8.3.0",
        "fakerphp/faker": "^1.9.1",
        "filament/filament": "^2.10",
        "filament/spatie-laravel-media-library-plugin": "^2.10",
        "filament/spatie-laravel-settings-plugin": "^2.10",
        "flowframe/laravel-trend": "^0.1.3",
        "geoip2/geoip2": "^2.12",
        "guzzlehttp/guzzle": "^7.5",
        "humaidem/filament-map-picker": "^0.1.3",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.9",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.0",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mbezhanov/faker-provider-collection": "^2.0",
        "mstaack/laravel-postgis": "dev-main",
        "paulhennell/faker-news": "^2.0",
        "phpunit/phpunit": "^10.0",
        "protoqol/prequel": "dev-main",
        "sentry/sentry-laravel": "^3.0",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-sluggable": "^3.3",
        "squirephp/countries-en": "^3.3",
        "squirephp/regions-en": "^3.3",
        "stevebauman/purify": "^5.0",
        "studio/laravel-totem": "^10.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "torann/geoip": "^3.0",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.0",
        "ysfkaya/filament-phone-input": "^1.2"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "barryvdh/laravel-ide-helper": "^2.11",
        "brianium/paratest": "^7.0",
        "driftingly/rector-laravel": "^0.16",
        "ergebnis/composer-normalize": "^2.28",
        "infection/infection": "^0.26.16",
        "itsgoingd/clockwork": "^5.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "phpstan/phpstan": "^1.4",
        "qossmic/deptrac-shim": "^1.0",
        "rector/rector": "^0.15",
        "roave/security-advisories": "dev-latest",
        "slevomat/coding-standard": "^8.0",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vkcom/modulite-phpstan": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:InfluxOW/Prequel.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:InfluxOW/baum.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:InfluxOW/laravel-postgis.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:InfluxOW/laravel-postgresql-database.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "bamarni/composer-bin-plugin": true,
            "infection/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 0,
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true,
            "target-directory": "tools"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"copy('.githooks/pre-commit', '.git/hooks/pre-commit');\"",
            "@php artisan livewire:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan filament:upgrade",
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
