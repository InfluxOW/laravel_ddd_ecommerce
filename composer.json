{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "akaunting/laravel-money": "^1.2",
        "baum/baum": "v2.x-dev",
        "darkaonline/l5-swagger": "^8.2.0",
        "fakerphp/faker": "^1.9.1",
        "filament/filament": "^2.10",
        "filament/spatie-laravel-media-library-plugin": "^2.10",
        "filament/spatie-laravel-settings-plugin": "^2.10",
        "flowframe/laravel-trend": "^0.0.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4.1",
        "laravel/framework": "^8.83",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^1.0",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-sluggable": "^3.3",
        "squirephp/countries-en": "^3.3",
        "squirephp/regions-en": "^3.2",
        "zircote/swagger-php": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.11",
        "facade/ignition": "^2.5",
        "itsgoingd/clockwork": "^5.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5.10",
        "roave/security-advisories": "dev-latest",
        "slevomat/coding-standard": "^7.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php -r \"copy('.githooks/pre-commit', '.git/hooks/pre-commit');\"",
            "@php -r \"@mkdir('./vendor/spatie/laravel-package-tools/database/factories', 0777, true);\"",
            "@php artisan livewire:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
